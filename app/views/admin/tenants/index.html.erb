

    <div id="page-wrapper">
      <div class="row">
        <div class="col-lg-8">

              
              <table id="tenants" class="table table-striped table-bordered">
                <thead>
                  <tr>
                    <th>Account No.</th> 
                    <th>Names</th>
                    <th>Property</th>
                    <th>Unit</th>
                    <th>Phone</th>
                    <th>Status</th>
                    <th>Created At</th>
                    <th>Action</th>
                    <th>Click for</th>
                  </tr>
                </thead>
                <tbody>
                  <% @tenants.each do |t|  %> 
                    <tr>
                      <td><%= t.id %></td>
                      <td><%= t.fname %>  <%= t.lname %></td>
                      <td><%= t.town %></td>
                      <td>property</td>
                      <td><%= t.phone %></td>
                      <td><%= t.status %></td>
                      <td><%= t.created_at.strftime("%Y-%m-%d") %></td>
                      <td><%= link_to 'Show', tenant_path(t), method: :get %></td>
                      <td><%= link_to 'Edit', edit_tenant_path(t), method: :get %></td>
                                         </tr>
                  <% end %>
                </tbody> 
              </table>

<script>
$(document).ready(function() {
$('#tenants').dataTable();

} );
</script>
        
       </div>
        <!-- /.col-lg-8 -->

        <div class="col-lg-4">
          
                      
        </div>
        <!-- /.col-lg-4 -->
      </div>
      <!-- /.row -->
    </div>
    <!-- /#page-wrapper -->