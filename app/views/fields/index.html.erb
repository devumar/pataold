<%= form_tag import_from_excel_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Import", :class=> "btn"%> 
<% end %>

<h1>Field transactions listing</h1>

<div class="table-responsive">
      <table id="landlords" class="table table-striped table-bordered">

  <thead>
    <tr>
      <th>Names</th>
      <th>Account No</th>
      <th>Property No</th>
      <th>Plot Number</th>
      <th>Unit No</th>
      <th>Amount</th>
      <th>Receipt</th>
      <th>Mode</th>
      <th colspan="3"></th>    
      
        

    </tr>
  </thead>

  <tbody>
    <% @fields.each do |f| %>
      <tr>
        <td>names</td>
         <td><%= f.first_name %></td>
         <td><%= f.last_name %></td>
        <td><%= f.plot_no %></td>
        <td><%= f.unit_no %></td>
        <td><%= f.amount %></td>
        <td><%= f.receipt %></td>
        <td><%= f.mode %></td>


        
        </td>
      </tr>
    <% end %>
  </div> 
  </tbody>
</table>